@page
@model IndexModel
@{
    ViewData["Title"] = "Inicio Mascota Feliz";
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width; initial-scale=1.0, maximun-scale=1">
    <!--etiqueta diseño adaptable-->
    <meta name="keywords" content="Veterinaria, mascotas, domicilios, perros, gatos, veterinarios" />
    <title>MascotaFeliz</title>
    <script src="uitotop-jquery-plugin.js" type="text/javascript"></script>
    <script src="jquery-3.1.0.min.js" type="text/javascript"> </script>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans|Open+sans|Oswald|Prompt|Arimo:400,700,400italic,700italic" />
    <link rel="stylesheet" href="~/css/mascotafeliz.css" type="text/css" media="screen and (min-width: 1025px) and (max-width: 1920px)" />
    <link rel="stylesheet" href="~/css/medium.css" type="text/css" media="screen and (min-width: 721px) and (max-width: 1024px)" />
    <link rel="stylesheet" href="~/css/mini.css" type="text/css" media="screen and (max-width: 720px)" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">

</head>


<main class="main">
    <div id="home">
        <img id="image_home" src="Imagenes/Home1.png" alt="image_home" />
        <h2>VETERINARIA MASCOTA FELIZ</h2>
        
        <img id="image_hometransp" src="Imagenes/HomeTransparencia.png" alt="image_Transparencia" />
    
    </div>
   
    <aside id="menu_huellas" role="complementary">
        <div id="acceso_veterinarios">
            <h2>ACCESO VETERINARIOS</h2>
            <article id="huella1"></article>
            <a href="/PagesVeterinarios/_AccesoVeterinarios" target="_self"><img id="imagen_huella1" src="Imagenes/huella.png" alt="image_Huella" />
            <br>
        </div>

        <div id="acceso_clientes">
            <h2>ACCESO CLIENTES</h2>
            <article id="huella2"></article>
            <a href="/PagesClientes/_AccesoClientes" target="_self"><img id="imagen_huella2" src="Imagenes/huella.png" alt="image_Huella" />
            <br>
        </div>

        <div id="cuidados_mascotas">
            <h2>CUIDADOS PARA TU MASCOTA</h2>
            <article id="huella3"></article>
            <a href="/PagesCuidados/_AccesoCuidados" target="_self"></a><img id="imagen_huella3" src="Imagenes/huella.png" alt="image_Huella" />
            <br>
        </div>

        <div id="acceso_internos">
            <h2>ACCESO INTERNO</h2>
            <article id="huella4"></article>
            <a href="/PagesAccesoInterno/_AccesoInterno" target="_self"></a><img id="imagen_huella4" src="Imagenes/huella.png" alt="image_Huella" />
            <br>
        </div>
    </aside>
</main>



</html>
